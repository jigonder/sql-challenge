    "# average salary bar graph\n",
    "\n",
    "avg_salary_df = final_df.groupby(final_df[\"Title\"])[\"salary\"].mean()\n",
    "avg_salary_df.plot(kind = \"bar\", legend=True)\n",
    "plt.xlabel(\"Job Title\")\n",
    "plt.xticks(rotation = 45)\n",
    "plt.ylabel(\"Average Salary\")\n",
    "plt.show()\n",
